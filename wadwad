repeat wait() until game:IsLoaded()
     

local function F(x)
    if x then
        for _, v in next, getconnections(x.Activated) do
            v.Function()
        end
    end
end

if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("LoadingGui") and game:GetService("Players").LocalPlayer.PlayerGui.LoadingGui.PlayBackground.Visible then
    if getgenv().Configs then
        if getgenv().Configs["Team"] == "Floppa" then
            repeat
                task.wait(1)
                F(game:GetService("Players").LocalPlayer.PlayerGui.LoadingGui.PlayBackground.Floppa.Select)
            until game:GetService("Players").LocalPlayer.PlayerGui.LoadingGui.PlayBackground.Floppa.Select.UIStroke.Color ~= Color3.fromRGB(255, 255, 255)
        elseif getgenv().Configs["Team"] == "Cheems" then
            repeat
                task.wait(1)
                F(game:GetService("Players").LocalPlayer.PlayerGui.LoadingGui.PlayBackground.Cheems.Select)
            until game:GetService("Players").LocalPlayer.PlayerGui.LoadingGui.PlayBackground.Cheems.Select.UIStroke.Color ~= Color3.fromRGB(255, 255, 255)
        end
    end
    F(game:GetService("Players").LocalPlayer.PlayerGui.LoadingGui.PlayBackground.Play)
end

