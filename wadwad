local function CheckQuest()
    local lv = game:GetService("Players").LocalPlayer.PlayerData.Level.Value
    for _, quest in next, quests do
        if lv >= quest.minLevel and lv <= quest.maxLevel then
            Mon = quest.mon
            NameQs = quest.nameQs
            break
        end
    end
end

local function Attack()
    xpcall(function()
        game:GetService("VirtualUser"):CaptureController()
        game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
    end,function () end)
end

local function handleMonster(v, targetCFrame)

    setscriptable(game.Players.LocalPlayer, "SimulationRadius", true)
    v:FindFirstChild("Humanoid").JumpPower = 0
    v:FindFirstChild("Humanoid").WalkSpeed = 0
    v:FindFirstChild("HumanoidRootPart").CanCollide = false
    v:FindFirstChild("Humanoid"):ChangeState(11)
    if targetCFrame == "Boss" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * getgenv().FarmPositionCFrame
        Attack()
    else
        v:FindFirstChild("HumanoidRootPart").CFrame = targetCFrame
    end
    sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", 1000000)

    if v:FindFirstChild("Humanoid"):FindFirstChild("Animator") then
        v:FindFirstChild("Humanoid"):FindFirstChild("Animator"):Destroy()
    end

    if v:FindFirstChild("Humanoid").Health <= 0 then
        v:Destroy()
    end
end

local function AutoBoss(BossName, FloppaQuest, Items, AutoFARM, MobName, CFRAME, ProximityPrompt, BossQuest)

    if (not game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) and not workspace.Monster:FindFirstChild(BossName)) and game.Players.LocalPlayer.Items.ItemStorage:FindFirstChild(Items).Value == 0 then
        local FoundMob = false
        for _, v in next, workspace.Monster:GetChildren() do
            if v.Name == MobName and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("Humanoid").Health > 0 then
                FoundMob = true

                repeat
                    task.wait()

                    if BossName == "Lord Sus" then
                        LordSusBringMob = true
                    end
                    if BossName == "Giant Pumpkin" then
                        GiantPumpkinbBringMob = true
                    end
                    UseSkillSelectBoss = true
                    PosMonBoss = v:FindFirstChild("HumanoidRootPart").CFrame

                    handleMonster(v,"Boss")
                until game.Players.LocalPlayer.Items.ItemStorage:FindFirstChild(Items).Value > 0 or v:FindFirstChild("Humanoid").Health <= 0 or not v.Parent or not getgenv()[AutoFARM] or game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) or workspace.Monster:FindFirstChild(BossName) or getgenv().SelectBoss ~= BossName or getgenv().AutoGetQuest
                    LordSusBringMob = false
                    GiantPumpkinbBringMob = false
                    UseSkillSelectBoss = false
                if v:FindFirstChild("Humanoid").Health <= 0 then
                    v:Destroy()
                end
                return
            end
        end

        if not FoundMob then
            local questNpc = workspace.Location.QuestLocaion:FindFirstChild(FloppaQuest)
            if questNpc then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = questNpc.CFrame * CFrame.new(20, 20, 20)
                return
            end
        end

    elseif game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) then

            local v = game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName)
            if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("Humanoid").Health > 0 then
                repeat
                    task.wait()
                    UseSkillSelectBoss = true
                    handleMonster(v,"Boss")
                until v:FindFirstChild("Humanoid").Health <= 0 or not getgenv()[AutoFARM] or not v.Parent or getgenv().SelectBoss ~= BossName or getgenv().AutoGetQuest
                UseSkillSelectBoss = false
                if v:FindFirstChild("Humanoid").Health <= 0 then
                    v:Destroy()
                end
                return
            end


    elseif (not game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) and not workspace.Monster:FindFirstChild(BossName)) and game.Players.LocalPlayer.Items.ItemStorage:FindFirstChild(Items).Value ~= 0 then

        repeat
            task.wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFRAME
        until (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CFRAME.Position).Magnitude < 5 or not getgenv()[AutoFARM] or getgenv().SelectBoss ~= BossName or getgenv().AutoGetQuest

        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CFRAME.Position).Magnitude < 5 then
            fireproximityprompt(ProximityPrompt)
        end

        if game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) then
            local v = game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName)
                if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("Humanoid").Health > 0 then
                    repeat
                        task.wait()
                        UseSkillSelectBoss = true
                        handleMonster(v,"Boss")
                    until v:FindFirstChild("Humanoid").Health <= 0 or not getgenv()[AutoFARM] or not v.Parent or getgenv().SelectBoss ~= BossName or getgenv().AutoGetQuest
                    UseSkillSelectBoss = false
                    if v:FindFirstChild("Humanoid").Health <= 0 then
                        v:Destroy()
                    end
                    return
                else
                    local questLocation = workspace.Location.QuestLocaion:FindFirstChild(BossQuest)
                    if questLocation then
                        local questNpc = workspace.NPCs.Quests_Npc:FindFirstChild(BossQuest)
                        if questNpc and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - questNpc.Block.Position).Magnitude < 5 then
                            fireproximityprompt(questNpc.Block.QuestPrompt)
                            wait(0.1)
                        else
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = questLocation.CFrame * CFrame.new(0, 4, 0)
                        end
                    end
                    return
                end
        end
    elseif workspace.Monster:FindFirstChild(BossName) then
            local v = workspace.Monster:FindFirstChild(BossName)
            if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("Humanoid").Health > 0 then
                repeat
                    task.wait()
                    UseSkillSelectBoss = true
                    handleMonster(v,"Boss")
                until v:FindFirstChild("Humanoid").Health <= 0 or not getgenv()[AutoFARM] or not v.Parent or getgenv().SelectBoss ~= BossName  or getgenv().AutoGetQuest
                UseSkillSelectBoss = false
                if v:FindFirstChild("Humanoid").Health <= 0 then
                    v:Destroy()
                end
                return
            end
    end
end

local function AutoBossQuest(BossName, FloppaQuest, Items, AutoFARM, MobName, CFRAME, ProximityPrompt, BossQuest)

    local function FindQuestLoction()
        local questLocation = workspace.Location.QuestLocaion:FindFirstChild(BossQuest)

        if questLocation then
            local questNpc = workspace.NPCs.Quests_Npc:FindFirstChild(BossQuest)
            if questNpc and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - questNpc.Block.Position).Magnitude < 5 then
                fireproximityprompt(questNpc.Block.QuestPrompt)
                wait(0.1)
            else
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = questLocation.CFrame * CFrame.new(0, 4, 0)
            end
        end
        return
    end

    if game.Players.LocalPlayer.PlayerGui.QuestGui.Holder.QuestSlot1.QuestGiver.Text ~= BossQuest then
        game.ReplicatedStorage.OtherEvent.QuestEvents.Quest:FireServer("Abandon_Quest", { QuestSlot = "QuestSlot1" })
    end

    if (not game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) and not workspace.Monster:FindFirstChild(BossName)) and game.Players.LocalPlayer.Items.ItemStorage:FindFirstChild(Items).Value == 0 then
        local FoundMob = false
        for _, v in next, workspace.Monster:GetChildren() do
            if v.Name == MobName and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("Humanoid").Health > 0 then
                FoundMob = true

                repeat
                    task.wait()

                    if BossName == "Lord Sus" then
                        LordSusBringMob = true
                    end
                    if BossName == "Giant Pumpkin" then
                        GiantPumpkinbBringMob = true
                    end
                    UseSkillSelectBoss = true
                    PosMonBoss = v:FindFirstChild("HumanoidRootPart").CFrame

                    handleMonster(v,"Boss")
                until game.Players.LocalPlayer.Items.ItemStorage:FindFirstChild(Items).Value > 0 or v:FindFirstChild("Humanoid").Health <= 0 or not v.Parent or not getgenv()[AutoFARM] or game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) or workspace.Monster:FindFirstChild(BossName) or getgenv().SelectBoss ~= BossName
                    LordSusBringMob = false
                    GiantPumpkinbBringMob = false
                    UseSkillSelectBoss = false
                if v:FindFirstChild("Humanoid").Health <= 0 then
                    v:Destroy()
                end
                return
            end
        end

        if not FoundMob then
            local questNpc = workspace.Location.QuestLocaion:FindFirstChild(FloppaQuest)
            if questNpc then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = questNpc.CFrame * CFrame.new(20, 20, 20)
                return
            end
        end

    elseif game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) then

        if game.Players.LocalPlayer.PlayerGui.QuestGui.Holder.QuestSlot1.Visible then
            local v = game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName)
            if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("Humanoid").Health > 0 then
                repeat
                    task.wait()
                    UseSkillSelectBoss = true
                    handleMonster(v,"Boss")
                until v:FindFirstChild("Humanoid").Health <= 0 or not getgenv()[AutoFARM] or not v.Parent or getgenv().SelectBoss ~= BossName or not game.Players.LocalPlayer.PlayerGui.QuestGui.Holder.QuestSlot1.Visible
                UseSkillSelectBoss = false
                if v:FindFirstChild("Humanoid").Health <= 0 then
                    v:Destroy()
                end
                return
            end
        else
            FindQuestLoction()
        end

    elseif (not game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) and not workspace.Monster:FindFirstChild(BossName)) and game.Players.LocalPlayer.Items.ItemStorage:FindFirstChild(Items).Value ~= 0 then

        repeat
            task.wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFRAME
        until (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CFRAME.Position).Magnitude < 5 or not getgenv()[AutoFARM] or getgenv().SelectBoss ~= BossName

        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CFRAME.Position).Magnitude < 5 then
            fireproximityprompt(ProximityPrompt)
        end

        if game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) then
            if game.Players.LocalPlayer.PlayerGui.QuestGui.Holder.QuestSlot1.Visible then
            local v = game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName)
                if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("Humanoid").Health > 0 then
                    repeat
                        task.wait()
                        UseSkillSelectBoss = true
                        handleMonster(v,"Boss")
                    until v:FindFirstChild("Humanoid").Health <= 0 or not getgenv()[AutoFARM] or not v.Parent or getgenv().SelectBoss ~= BossName or not game.Players.LocalPlayer.PlayerGui.QuestGui.Holder.QuestSlot1.Visible
                    UseSkillSelectBoss = false
                    if v:FindFirstChild("Humanoid").Health <= 0 then
                        v:Destroy()
                    end
                    return
                else
                    FindQuestLoction()
                end
            end
        end
    elseif workspace.Monster:FindFirstChild(BossName) then
        if game.Players.LocalPlayer.PlayerGui.QuestGui.Holder.QuestSlot1.Visible then
            local v = workspace.Monster:FindFirstChild(BossName)
            if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("Humanoid").Health > 0 then
                repeat
                    task.wait()
                    UseSkillSelectBoss = true
                    handleMonster(v,"Boss")
                until v:FindFirstChild("Humanoid").Health <= 0 or not getgenv()[AutoFARM] or not v.Parent or getgenv().SelectBoss ~= BossName or not game.Players.LocalPlayer.PlayerGui.QuestGui.Holder.QuestSlot1.Visible
                UseSkillSelectBoss = false
                if v:FindFirstChild("Humanoid").Health <= 0 then
                    v:Destroy()
                end
                return
            end
        else
            FindQuestLoction()
        end
    end
end

local function AutoSword(BossName, FloppaQuest, Items, AutoFARM, MobName, CFRAME, ProximityPrompt, BossQuest)

    if (not game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) and not workspace.Monster:FindFirstChild(BossName)) and game.Players.LocalPlayer.Items.ItemStorage:FindFirstChild(Items).Value == 0 then
        local FoundMob = false
        for _, v in next, workspace.Monster:GetChildren() do
            if v.Name == MobName and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("Humanoid").Health > 0 then
                FoundMob = true

                repeat
                    task.wait()

                    UseSkillAutoSword = true
                    if BossName == "Lord Sus" then
                        LordSusBringMob = true
                    end
                    if BossName == "Giant Pumpkin" then
                        GiantPumpkinbBringMob = true
                    end

                    PosMonBoss = v:FindFirstChild("HumanoidRootPart").CFrame

                    handleMonster(v,"Boss")
                until game.Players.LocalPlayer.Items.ItemStorage:FindFirstChild(Items).Value > 0 or v:FindFirstChild("Humanoid").Health <= 0 or not v.Parent or not getgenv()[AutoFARM] or game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) or workspace.Monster:FindFirstChild(BossName)
                    LordSusBringMob = false
                    GiantPumpkinbBringMob = false
                    UseSkillAutoSword = false
                if v:FindFirstChild("Humanoid").Health <= 0 then
                    v:Destroy()
                end
                return
            end
        end

        if not FoundMob then
            local questNpc = workspace.Location.QuestLocaion:FindFirstChild(FloppaQuest)
            if questNpc then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = questNpc.CFrame * CFrame.new(20, 20, 20)
                return
            end
        end

    elseif game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) then

            local v = game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName)
            if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("Humanoid").Health > 0 then
                repeat
                    task.wait()

                    UseSkillAutoSword = true
                    handleMonster(v,"Boss")
                until v:FindFirstChild("Humanoid").Health <= 0 or not getgenv()[AutoFARM] or not v.Parent 
                UseSkillAutoSword = false
                if v:FindFirstChild("Humanoid").Health <= 0 then
                    v:Destroy()
                end
                return
            end

    elseif (not game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) and not workspace.Monster:FindFirstChild(BossName)) and game.Players.LocalPlayer.Items.ItemStorage:FindFirstChild(Items).Value ~= 0 then

        repeat
            task.wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFRAME
        until (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CFRAME.Position).Magnitude < 5 or not getgenv()[AutoFARM]

        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CFRAME.Position).Magnitude < 5 then
            fireproximityprompt(ProximityPrompt)
        end

        if game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName) then
            if game.Players.LocalPlayer.PlayerGui.QuestGui.Holder.QuestSlot1.Visible then
            local v = game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(BossName)
                    repeat
                        task.wait()
                        UseSkillAutoSword = true
                        handleMonster(v,"Boss")
                    until v:FindFirstChild("Humanoid").Health <= 0 or not getgenv()[AutoFARM] or not v.Parent
                    UseSkillAutoSword = false
                    if v:FindFirstChild("Humanoid").Health <= 0 then
                        v:Destroy()
                    end
                    return
                end
        end
    elseif workspace.Monster:FindFirstChild(BossName) then
            local v = workspace.Monster:FindFirstChild(BossName)
            if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v:FindFirstChild("Humanoid").Health > 0 then
                repeat
                    task.wait()
                    UseSkillAutoSword = true
                    handleMonster(v,"Boss")
                until v:FindFirstChild("Humanoid").Health <= 0 or not getgenv()[AutoFARM] or not v.Parent 
                UseSkillAutoSword = false
                if v:FindFirstChild("Humanoid").Health <= 0 then
                    v:Destroy()
                end
                return
            end
    end
end

local function EquipToolByName(toolName)
    local player = game:GetService("Players").LocalPlayer
    local tool = player.Backpack:FindFirstChild(toolName)

    if tool and toolName then
        player.Character.Humanoid:EquipTool(tool)
    end
end

local function simulateKeyPress(Key)
    local virtualInput = game:GetService("VirtualInputManager")
    virtualInput:SendKeyEvent(true, Key, false, game)
    wait(0.2)
    virtualInput:SendKeyEvent(false, Key, false, game)
end

local function FireButton(x)
    if x then
        for _, v in next, getconnections(x.Activated) do
            v.Function()
        end
    end
end

local function getClosestMob()
    local closestMob = nil
    local shortestDistance = getgenv().Rages

    for _, mob in next, workspace.Monster:GetChildren() do
        if mob.Name ~= "Training Log" and mob.Parent and mob:FindFirstChild("HumanoidRootPart") and mob:FindFirstChild("Humanoid") and mob:FindFirstChild("Humanoid").Health > 0 then
            local distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - mob.HumanoidRootPart.Position)
            .magnitude

            if distance < shortestDistance then
                shortestDistance = distance
                closestMob = mob
            end
        end
    end

    return closestMob
end

local function AutoFarm(Global)
    CheckQuest()
    local player = game.Players.LocalPlayer
    local questGui = player.PlayerGui.QuestGui.Holder.QuestSlot1
    local questGiverText = questGui.QuestGiver.Text

    if getgenv()[Global] then
        if game.Players.LocalPlayer.PlayerGui.QuestGui.Holder.QuestSlot1.QuestGiver.Text ~= NameQs then
            game.ReplicatedStorage.OtherEvent.QuestEvents.Quest:FireServer("Abandon_Quest",{ QuestSlot = "QuestSlot1" })
        end

        if game.Players.LocalPlayer.PlayerGui.QuestGui.Holder.QuestSlot1.Visible then
            for _, v in next, workspace.Location.QuestLocaion:GetChildren() do
                if v:IsA("Part") and v.Name == NameQs then
                    local monster = workspace.Monster:FindFirstChild(Mon)

                    if monster and monster:FindFirstChild("HumanoidRootPart") and monster:FindFirstChild("Humanoid").Health > 0 then
                        repeat
                            task.wait()

                            BringMobFarm = true
                            UseSkillAutoFarm = true
                            MonPosFarm = monster.HumanoidRootPart.CFrame

                            Attack()

                            player.Character.HumanoidRootPart.CFrame = monster.HumanoidRootPart.CFrame * getgenv().FarmPositionCFrame
                        until not monster.Parent or not questGui.Visible or monster.Humanoid.Health <= 0 or not getgenv()[Global] or questGiverText ~= NameQs

                        BringMobFarm = false
                        UseSkillAutoFarm = false

                        if monster.Humanoid.Health <= 0 then
                            monster:Destroy()
                        end
                        return
                    else
                        local questLocation = workspace.Location.QuestLocaion:FindFirstChild(NameQs)
                        if questLocation then
                            player.Character.HumanoidRootPart.CFrame = questLocation.CFrame * CFrame.new(0, 4, 0)
                            return
                        end
                    end
                end
            end
        else
            local questLocation = workspace.Location.QuestLocaion:FindFirstChild(NameQs)
            if questLocation then
                local questNpc = workspace.NPCs.Quests_Npc:FindFirstChild(NameQs)
                if questNpc and (player.Character.HumanoidRootPart.Position - questNpc.Block.Position).Magnitude < 5 then
                    fireproximityprompt(questNpc.Block.QuestPrompt)
                    wait(0.1)
                else
                    player.Character.HumanoidRootPart.CFrame = questLocation.CFrame * CFrame.new(0, 4, 0)
                end
            end
            return
        end
    end
end

local function Raid(Global)
    if getgenv()[Global] then
        if not game:GetService("Players").LocalPlayer.PlayerGui.RaidGui.RaidFrame.Visible then
            if (game:GetService("Workspace").Region.RaidArea.Position -  game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude > 5 then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Region.RaidArea.CFrame
            end
        else
            if game:GetService("Players").LocalPlayer.PlayerGui.RaidGui.RaidFrame.Start_Button.Visible then
                repeat task.wait() FireButton(game:GetService("Players").LocalPlayer.PlayerGui.RaidGui.RaidFrame.Start_Button) until not game:GetService("Players").LocalPlayer.PlayerGui.RaidGui.RaidFrame.Start_Button.Visible or not getgenv()[Global]
            elseif game:GetService("Players").LocalPlayer.PlayerGui.RaidGui.RaidFrame.TimeLeft.Visible then
                for _, raid in next, workspace.Raids:GetChildren() do
                    if raid.Joiners:FindFirstChild(game.Players.LocalPlayer.Name) then
                        if (raid.Joiners:FindFirstChild(game.Players.LocalPlayer.Name).Parent.Parent:GetChildren()[2].Position -  game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 1000 then
                            for _,v in next, game.Workspace.Monster:GetChildren() do
                                if v and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid").Health > 0 and (raid.Joiners:FindFirstChild(game.Players.LocalPlayer.Name).Parent.Parent:GetChildren()[2].Position - v:FindFirstChild("HumanoidRootPart").Position).Magnitude < 1000 then
                                    repeat
                                        task.wait()
                                    
                                        BringMobRaid = true
                                        UseSkillRaid = true
                                        MonPosRaid = v:FindFirstChild("HumanoidRootPart").CFrame
                                    
                                        Attack()
                                    
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * getgenv().FarmPositionCFrame 
                                    until not v.Parent or v.Humanoid.Health <= 0 or not getgenv()[Global] or (raid.Joiners:FindFirstChild(game.Players.LocalPlayer.Name).Parent.Parent:GetChildren()[2].Position -  game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude > 1000
                                    
                                    BringMobRaid = false
                                    UseSkillRaid = false
                                    if v.Humanoid.Health <= 0 then
                                        v:Destroy()
                                    end
                                    return
                                end
                            end
                        else
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = raid.Joiners:FindFirstChild(game.Players.LocalPlayer.Name).Parent.Parent:GetChildren()[2].CFrame
                        end
                    end
                end
            end
        end
    end
end

------------------------------------ Main

task.spawn(function() -- AutoFarm
    while wait() do
        xpcall(function()
            AutoFarm("AutoFarm")
        end, function() end)
    end
end)

task.spawn(function() -- AutoFarmNearByMobs
    while wait() do
        xpcall(function()
            if getgenv().AutoFarmNearByMobs then
                local v = getClosestMob()
                if v and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid").Health > 0 then
                    repeat
                        task.wait()

                        BringMobNear = true
                        UseSkillNearByMobs = true
                        MonPosNear = v:FindFirstChild("HumanoidRootPart").CFrame

                        Attack()

                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * getgenv().FarmPositionCFrame
                    until not v.Parent or v.Humanoid.Health <= 0 or not getgenv().AutoFarmNearByMobs
                    UseSkillNearByMobs = false
                    BringMobNear = false

                    if v.Humanoid.Health <= 0 then
                        v:Destroy()
                    end
                    return
                end
            end
        end, function() end)
    end
end)

task.spawn(function() -- AutoFarmMobsSelect
    while task.wait() do
        xpcall(function()
            if getgenv().AutoFarmMobsSelect then
                local foundMob = false

                for _, mob in pairs(workspace.Monster:GetChildren()) do
                    if table.find(getgenv().SelectMobs, mob.Name) and mob.Parent and mob:FindFirstChild("Humanoid") and mob.Humanoid.Health > 0 then
                        foundMob = true
                        MonPosSelect = mob.HumanoidRootPart.CFrame

                        repeat
                            task.wait()
                            UseSkillMobSelect = true
                            BringMobSelect = true

                            Attack()

                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = MonPosSelect *
                            getgenv().FarmPositionCFrame
                        until not table.find(getgenv().SelectMobs, mob.Name) or not mob.Parent or mob.Humanoid.Health <= 0 or not getgenv().AutoFarmMobsSelect
                        UseSkillMobSelect = false
                        BringMobSelect = false

                        if mob.Humanoid.Health <= 0 then
                            mob:Destroy()
                        end

                        return
                    end
                end

                if not foundMob then
                    for _, quest in next, quests do
                        if table.find(getgenv().SelectMobs, quest.mon) then
                            local questNpc = workspace.Location.QuestLocaion:FindFirstChild(quest.nameQs)
                            if questNpc then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = questNpc.CFrame *
                                CFrame.new(20, 20, 20)
                            end
                        end
                    end
                end
            end
        end, function() end)
    end
end)

task.spawn(function() -- BringMobs
    while task.wait() do
        xpcall(function()
            if getgenv().BringMob then
                CheckQuest()
                if BringMobSelect and getgenv().AutoFarmMobsSelect then
                    for _, v in next, workspace.Monster:GetChildren() do
                        local humanoid = v:FindFirstChild("Humanoid")
                        local humanoidRootPart = v:FindFirstChild("HumanoidRootPart")
                        if table.find(getgenv().SelectMobs, v.Name) and humanoid and humanoidRootPart and humanoid.Health > 0 and game.Players.LocalPlayer:DistanceFromCharacter(v:GetPivot().Position) <= 255 then
                            handleMonster(v, MonPosSelect)
                        end
                    end
                elseif BringMobNear and getgenv().AutoFarmNearByMobs then
                    for _, v in next, workspace.Monster:GetChildren() do
                        local humanoid = v:FindFirstChild("Humanoid")
                        local humanoidRootPart = v:FindFirstChild("HumanoidRootPart")
                        if v.Name ~= "Training Log" and humanoid and humanoidRootPart and humanoid.Health > 0 and game.Players.LocalPlayer:DistanceFromCharacter(v:GetPivot().Position) <= 255 then
                            handleMonster(v, MonPosNear)
                        end
                    end
                elseif BringMobFarm and (getgenv().AutoFarm or getgenv().AutoAbility) then
                    for _, v in next, workspace.Monster:GetChildren() do
                        local humanoid = v:FindFirstChild("Humanoid")
                        local humanoidRootPart = v:FindFirstChild("HumanoidRootPart")
                        if v.Name == Mon and humanoid and humanoidRootPart and humanoid.Health > 0 and game.Players.LocalPlayer:DistanceFromCharacter(v:GetPivot().Position) <= 255 then
                            handleMonster(v, MonPosFarm)
                        end
                    end
                elseif LordSusBringMob and (getgenv().AutoFarmBossSelect or getgenv().AutoPurpleKatana) then
                    for _, v in next, workspace.Monster:GetChildren() do
                        local humanoid = v:FindFirstChild("Humanoid")
                        local humanoidRootPart = v:FindFirstChild("HumanoidRootPart")
                        if v.Name == "Sus Duck" and humanoid and humanoidRootPart and humanoid.Health > 0 and game.Players.LocalPlayer:DistanceFromCharacter(v:GetPivot().Position) <= 255 then
                            handleMonster(v, PosMonBoss)
                        end
                    end
                elseif GiantPumpkinbBringMob and getgenv().AutoFarmBossSelect then
                    for _, v in next, workspace.Monster:GetChildren() do
                        local humanoid = v:FindFirstChild("Humanoid")
                        local humanoidRootPart = v:FindFirstChild("HumanoidRootPart")
                        if v.Name == "Scary Skull" and humanoid and humanoidRootPart and humanoid.Health > 0 and game.Players.LocalPlayer:DistanceFromCharacter(v:GetPivot().Position) <= 255 then
                            handleMonster(v, PosMonBoss)
                        end
                    end
                elseif BringMobAura and getgenv().AutoFarmLevelAbility then
                    for _, v in next, workspace.Monster:GetChildren() do
                        local humanoid = v:FindFirstChild("Humanoid")
                        local humanoidRootPart = v:FindFirstChild("HumanoidRootPart")
                        if v.Name == Mon and humanoid and humanoidRootPart and humanoid.Health > 0 and game.Players.LocalPlayer:DistanceFromCharacter(v:GetPivot().Position) <= 255 then
                            handleMonster(v, MonPosAura)
                        end
                    end
                elseif BringMobRaid and getgenv().AutoRaid then
                    for _, v in next, workspace.Monster:GetChildren() do
                        local humanoid = v:FindFirstChild("Humanoid")
                        local humanoidRootPart = v:FindFirstChild("HumanoidRootPart")
                        if humanoid and humanoidRootPart and humanoid.Health > 0 and game.Players.LocalPlayer:DistanceFromCharacter(v:GetPivot().Position) <= 255 then
                            handleMonster(v, MonPosRaid)
                        end
                    end
                end
            end
        end, function() end)
    end
end)

task.spawn(function() -- AutoFarmBossSelect
    while wait() do
        xpcall(function()
            if getgenv().AutoFarmBossSelect then
                if getgenv().SelectBoss == "Evil Noob"  then
                    if getgenv().AutoGetQuest then
                        AutoBossQuest("Evil Noob","Floppa Quest 28","Noob Head","AutoFarmBossSelect","Moai",CFrame.new(-2356, -81, 3178),workspace.Island.MoaiIsland.Summon2.Summon.SummonPrompt,"Floppa Quest 29")
                    else
                        AutoBoss("Evil Noob","Floppa Quest 28","Noob Head","AutoFarmBossSelect","Moai",CFrame.new(-2356, -81, 3178),workspace.Island.MoaiIsland.Summon2.Summon.SummonPrompt,"Floppa Quest 29")
                    end
                end
                if getgenv().SelectBoss == "Lord Sus" then
                    if getgenv().AutoGetQuest then
                        AutoBossQuest("Lord Sus","Floppa Quest 31","Sussy Orb","AutoFarmBossSelect","Sus Duck",CFrame.new(6642, -95, 4810),workspace.Island.ForgottenIsland.Summon3.Summon.SummonPrompt,"Floppa Quest 32")
                    else
                        AutoBoss("Lord Sus","Floppa Quest 31","Sussy Orb","AutoFarmBossSelect","Sus Duck",CFrame.new(6642, -95, 4810),workspace.Island.ForgottenIsland.Summon3.Summon.SummonPrompt,"Floppa Quest 32")
                    end
                end
                if getgenv().SelectBoss == "Giant Pumpkin" then
                    if getgenv().AutoGetQuest then
                        AutoBossQuest("Giant Pumpkin","Floppa Quest 22","Flame Orb","AutoFarmBossSelect","Scary Skull",CFrame.new(-1181, -93, 1460),workspace.Island.PumpkinIsland.Summon1.Summon.SummonPrompt,"Floppa Quest 23")
                    else
                        AutoBoss("Giant Pumpkin","Floppa Quest 22","Flame Orb","AutoFarmBossSelect","Scary Skull",CFrame.new(-1181, -93, 1460),workspace.Island.PumpkinIsland.Summon1.Summon.SummonPrompt,"Floppa Quest 23")
                    end
                end
                if getgenv().SelectBoss == "Meme Beast" then
                    if game:GetService("Workspace").Monster:FindFirstChild("Meme Beast") or game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild("Meme Beast") then
                    local v = game:GetService("Workspace").Monster:FindFirstChild("Meme Beast")
                        if v and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid").Health > 0 then
                            repeat
                                task.wait()
        
                                UseSkillMemeBeast = true
        
                                Attack()
        
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * getgenv().FarmPositionCFrame
                            until not v.Parent or v.Humanoid.Health <= 0 or not getgenv().AutoFarmBossSelect
                            UseSkillMemeBeast = false
        
                            if v.Humanoid.Health <= 0 then
                                v:Destroy()
                            end
                            return
                        end
                    else
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1617, 167, -2791)
                    end
                end
            end
        end,function () end)
    end
end)

task.spawn(function() -- Auto Sword
    while wait() do
        xpcall(function()
            if getgenv().AutoYellowBlade and game:GetService("Players").LocalPlayer.Items.Weapon["Yellow Blade"].Value == 0 then
                AutoSword("Evil Noob","Floppa Quest 28","Noob Head","AutoYellowBlade","Moai",CFrame.new(-2356, -81, 3178),workspace.Island.MoaiIsland.Summon2.Summon.SummonPrompt,"Floppa Quest 29")
            elseif getgenv().AutoPurpleKatana and game:GetService("Players").LocalPlayer.Items.Weapon["Purple Katana"].Value == 0 then
                AutoSword("Lord Sus","Floppa Quest 31","Sussy Orb","AutoPurpleKatana","Sus Duck",CFrame.new(6642, -95, 4810),workspace.Island.ForgottenIsland.Summon3.Summon.SummonPrompt,"Floppa Quest 32")
            elseif getgenv().AutoPortal and game:GetService("Players").LocalPlayer.Items.Weapon.Portal.Value == 0 then
                if game:GetService("Workspace").Monster:FindFirstChild("Meme Beast") or game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild("Meme Beast") then
                    local v = game:GetService("Workspace").Monster:FindFirstChild("Meme Beast")
                        if v and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid").Health > 0 then
                            repeat
                                task.wait()
        
                                UseSkillPortal  = true
        
                                Attack()
        
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * getgenv().FarmPositionCFrame
                            until not v.Parent or v.Humanoid.Health <= 0 or not getgenv().AutoFarmBossSelect
                            UseSkillPortal = false
        
                            if v.Humanoid.Health <= 0 then
                                v:Destroy()
                            end
                            return
                        end
                    else
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1617, 167, -2791)
                    end
            end
        end,function () end)
    end
end)

task.spawn(function() -- AutoBringFruits
	while task.wait() do
		pcall(function()
			if getgenv().AutoBringFruits then
				for i, v in next, game:GetService("Workspace").Dropped_Items:GetChildren() do
					if v and v.ToolTip == "Power" then
						firetouchinterest(game.Players.LocalPlayer.Character.Head, v.Handle, 0)
					end
				end
			end
		end)
	end
end)

task.spawn(function() -- AutoStoreFruits
    while task.wait() do
        pcall(function()
            if getgenv().AutoStoreFruits then
                for _, Fruits in next, game:GetService("Players").LocalPlayer.Items.Power:GetChildren() do
                    for i, v in next, game:GetService("Players").LocalPlayer.Backpack:GetChildren() do
                        if v:FindFirstChild("Script") and Fruits.Name == v.Name then
                            if Fruits.Value < 99 then
                            EquipToolByName(v.Name)
                            game:GetService("ReplicatedStorage"):WaitForChild("OtherEvent"):WaitForChild("MainEvents"):WaitForChild("Modules"):FireServer("Eatable_Power",{ ["Action"] = "Store", ["Tool"] = workspace.Character[game.Players.LocalPlayer.Name][tostring(v.Name)] })
                            end
                        end
                    end
                end
            end
        end)
    end
end)

task.spawn(function() -- AutoAbility
    while wait() do
        xpcall(function()
            if getgenv().AutoAbility then
                if getgenv().SelectAbility == "Aura" then
                    if not game:GetService("Players").LocalPlayer.Ability.Aura.Value then
                        if game:GetService("Players").LocalPlayer.Items.ItemStorage["Meme Cube"].Value >= 1 and game:GetService("Players").LocalPlayer.PlayerData.Money.Value >= 10000000 then
                            game:GetService("ReplicatedStorage"):WaitForChild("OtherEvent"):WaitForChild("MainEvents"):WaitForChild("Modules"):FireServer("Ability_Teacher","Aura Master")
                        elseif game:GetService("Players").LocalPlayer.Items.ItemStorage["Meme Cube"].Value >= 1 and game:GetService("Players").LocalPlayer.PlayerData.Money.Value < 10000000 then
                            AutoFarm("AutoAbility")
                        elseif game:GetService("Players").LocalPlayer.Items.ItemStorage["Meme Cube"].Value == 0 and game:GetService("Players").LocalPlayer.PlayerData.Money.Value >= 10000000 then
                            if game:GetService("Workspace").Monster:FindFirstChild("Meme Beast") or game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild("Meme Beast") then
                                local v = game:GetService("Workspace").Monster:FindFirstChild("Meme Beast")
                                    if v and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid").Health > 0 then
                                        repeat
                                            task.wait()
                    
                                            UseSkillMemeBeast = true
                    
                                            Attack()
                    
                                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * getgenv().FarmPositionCFrame
                                        until not v.Parent or v.Humanoid.Health <= 0 or not getgenv().AutoAbility
                                        UseSkillMemeBeast = false
                    
                                        if v.Humanoid.Health <= 0 then
                                            v:Destroy()
                                        end
                                        return
                                    end
                                else
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1617, 167, -2791)
                                end
                        elseif game:GetService("Players").LocalPlayer.Items.ItemStorage["Meme Cube"].Value == 0 and game:GetService("Players").LocalPlayer.PlayerData.Money.Value < 10000000 then
                            AutoFarm("AutoAbility")
                        end
                    end
                elseif getgenv().SelectAbility == "Flash Step" then
                    if not game:GetService("Players").LocalPlayer.Ability.FlashStep.Value then
                        if game:GetService("Players").LocalPlayer.PlayerData.Money.Value >= 100000 then
                            game:GetService("ReplicatedStorage"):WaitForChild("OtherEvent"):WaitForChild("MainEvents"):WaitForChild("Modules"):FireServer("Ability_Teacher","Giga Chad")
                        else
                            AutoFarm("AutoAbility")
                        end
                    end
                elseif getgenv().SelectAbility == "Instinct" then
                    if not game:GetService("Players").LocalPlayer.Ability.Instinct.Value then 
                        if game:GetService("Players").LocalPlayer.PlayerData.Money.Value >= 2500000 then
                            game:GetService("ReplicatedStorage"):WaitForChild("OtherEvent"):WaitForChild("MainEvents"):WaitForChild("Modules"):FireServer("Ability_Teacher","Nugget Man")
                        else
                            AutoFarm("AutoAbility")
                        end
                    end
                end
            end
        end, function() end)
    end
end)

task.spawn(function() -- AutoFarmLevelAbility
    while wait() do
        xpcall(function()
            if getgenv().AutoFarmLevelAbility then
                if getgenv().SelectAbility == "Aura" then
                    if game:GetService("Players").LocalPlayer.Ability.Aura.Value then
                        if #game.Players.LocalPlayer.Character.AuraColor_Folder:GetChildren() < 1 then
                            game.ReplicatedStorage:WaitForChild("OtherEvent").MainEvents.Ability:InvokeServer("Aura")
                        else
                            CheckQuest()
                            local player = game.Players.LocalPlayer

                            for _, v in next, workspace.Location.QuestLocaion:GetChildren() do
                                if v:IsA("Part") and v.Name == NameQs then
                                    local monster = workspace.Monster:FindFirstChild(Mon)

                                    if monster and monster:FindFirstChild("HumanoidRootPart") and monster:FindFirstChild("Humanoid").Health > 0 then
                                        MonPosAura = monster.HumanoidRootPart.CFrame
                                        repeat
                                            task.wait()
                                            UseSkillAura = true
                                            BringMobAura = true

                                            Attack()

                                            player.Character.HumanoidRootPart.CFrame = MonPosAura *
                                            getgenv().FarmPositionCFrame
                                        until not monster.Parent or monster.Humanoid.Health <= 0 or not getgenv().AutoFarmLevelAbility or getgenv().SelectAbility ~= "Aura"
                                        UseSkillAura = false
                                        BringMobAura = false

                                        if monster.Humanoid.Health <= 0 then
                                            monster:Destroy()
                                        end
                                        return
                                    else
                                        local questLocation = workspace.Location.QuestLocaion:FindFirstChild(NameQs)
                                        if questLocation then
                                            player.Character.HumanoidRootPart.CFrame = questLocation.CFrame *
                                                CFrame.new(0, 4, 0)
                                            return
                                        end
                                    end
                                end
                            end
                        end
                    end
                elseif getgenv().SelectAbility == "Flash Step" then
                    if game:GetService("Players").LocalPlayer.Ability.FlashStep.Value and not game:GetService("Players").LocalPlayer.Cooldown:FindFirstChild("FlashStepCD") then
                        game:GetService("ReplicatedStorage").OtherEvent.MainEvents.Ability:InvokeServer("FlashStep",
                            { ["Mouse"] = game.Players.LocalPlayer.Character.HumanoidRootPart.Position })
                    end
                elseif getgenv().SelectAbility == "Instinct" then
                    local player = game:GetService("Players").LocalPlayer

                    if game:GetService("Players").LocalPlayer.Ability.Instinct.Value then
                    if game.Players.LocalPlayer.Character:FindFirstChild("Highlight_Player") then
                        CheckQuest()

                        for _, v in next, workspace.Location.QuestLocaion:GetChildren() do
                            if v:IsA("Part") and v.Name == NameQs then
                                local monster = workspace.Monster:FindFirstChild(Mon)

                                if monster and monster:FindFirstChild("HumanoidRootPart") and monster:FindFirstChild("Humanoid").Health > 0 then

                                    repeat
                                        task.wait()

                                        Attack()

                                        MonPosInstinct = monster.HumanoidRootPart.CFrame

                                        player.Character.HumanoidRootPart.CFrame = MonPosInstinct
                                    until not monster.Parent or monster.Humanoid.Health <= 0 or not getgenv().AutoFarmLevelAbility or not game.Players.LocalPlayer.Character:FindFirstChild("Highlight_Player") or getgenv().SelectAbility ~= "Instinct"

                                    if not game.Players.LocalPlayer.Character:FindFirstChild("Highlight_Player") then
                                        repeat task.wait() player.Character.HumanoidRootPart.CFrame = MonPosInstinct * CFrame.new(0, 15, 0) until not game:GetService("Players").LocalPlayer.Cooldown:FindFirstChild("InstinctCD") or not getgenv().AutoFarmLevelAbility or getgenv().SelectAbility ~= "Instinct" or game.Players.LocalPlayer.Character.Humanoid.Health <= 0

                                        if not getgenv().AutoFarmLevelAbility or getgenv().SelectAbility ~= "Instinct" then return end

                                        game:GetService("VirtualInputManager"):SendKeyEvent(true, "E", false, player)
                                        game:GetService("VirtualInputManager"):SendKeyEvent(false, "E", false, player)
                                            repeat task.wait() until game:GetService("Players").LocalPlayer.PlayerData.Dodge.Value == game:GetService("Players").LocalPlayer.PlayerData.MaxDodge.Value or not getgenv().AutoFarmLevelAbility or getgenv().SelectAbility ~= "Instinct" or game.Players.LocalPlayer.Character.Humanoid.Health <= 0
                                        return
                                    end


                                    if monster.Humanoid.Health <= 0 then
                                        monster:Destroy()
                                    end
                                    return
                                else
                                    local questLocation = workspace.Location.QuestLocaion:FindFirstChild(NameQs)
                                    if questLocation then
                                        player.Character.HumanoidRootPart.CFrame = questLocation.CFrame *
                                            CFrame.new(0, 4, 0)
                                        return
                                    end
                                end
                            end
                        end
                    else
                        game:GetService("VirtualInputManager"):SendKeyEvent(true, "E", false, player)
                        game:GetService("VirtualInputManager"):SendKeyEvent(false, "E", false, player)
                        wait(0.5)
                        return
                    end
                    end
                end
            end
        end, function() end)
    end
end)

task.spawn(function() -- AutoRaid
    while wait() do
        xpcall(function()
            Raid("AutoRaid")
        end, function() end)
    end
end)

task.spawn(function() -- AutoUpStats
    while wait() do
        xpcall(function()
            if getgenv().AutoUpStats then
                local stats = {"Melee", "Health", "Weapon", "Power"}

                local function ReturnLevel(stat)
                    if stat == "Melee" then
                        return "MeleeLevel"
                    elseif stat == "Health" then
                        return "DefenseLevel"
                    elseif stat == "Weapon" then
                        return "SwordLevel"
                    elseif stat == "Power" then
                        return "MemePowerLevel"
                    end
                end

                for _, stat in ipairs(stats) do
                    if table.find(getgenv().SelectStats, stat) then
                        local target = ReturnLevel(stat)

                        game:GetService("ReplicatedStorage").OtherEvent.MainEvents.StatsFunction:InvokeServer({
                            ["Target"] = target,
                            ["Action"] = "UpgradeStats",
                            ["Amount"] = getgenv().Point
                        })
                    end
                end
            end
        end, function() end)
    end
end)

task.spawn(function() -- Auto Enable
	while wait(0.5) do
		pcall(function()
			if getgenv().AutoEnableAura then
				if #game.Players.LocalPlayer.Character.AuraColor_Folder:GetChildren() < 1 and not game:GetService("Players").LocalPlayer.Cooldown:FindFirstChild("AuraCD") then
					game.Players.LocalPlayer.Ability.Aura.Value = true
					game.ReplicatedStorage:WaitForChild("OtherEvent").MainEvents.Ability:InvokeServer("Aura")
				end
			end
			if getgenv().AutoEnableInstinct then
				if not game.Players.LocalPlayer.Character:FindFirstChild("Highlight_Player") and game.Players.LocalPlayer.Ability.Instinct.Value and not game:GetService("Players").LocalPlayer.Cooldown:FindFirstChild("InstinctCD") then
					game:GetService("VirtualInputManager"):SendKeyEvent(true, "E", false, game.Players.LocalPlayer)
					game:GetService("VirtualInputManager"):SendKeyEvent(false, "E", false, game)
				end
			end
			if getgenv().AutoEnableRace then
				game:GetService("ReplicatedStorage").OtherEvent.SkillEvents.Movement:FireServer("RaceSkill")
			end
		end)
	end
end)

task.spawn(function() -- BodyClip
    while task.wait() do
        xpcall(function()
            if getgenv().AutoFarm or getgenv().AutoFarmNearByMobs or getgenv().AutoFarmMobsSelect or getgenv().AutoYellowBlade or getgenv().AutoFarmBossSelect or getgenv().AutoPurpleKatana or (getgenv().AutoFarmLevelAbility and getgenv().SelectAbility ~= "Flash Step") or getgenv().AutoAbility or getgenv().AutoRaid or getgenv().AutoPortal then
                if not game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip") then
                    local Noclip = Instance.new("BodyVelocity")
                    Noclip.Name = "BodyClip"
                    Noclip.Parent = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
                    Noclip.MaxForce = Vector3.new(100000, 100000, 100000)
                    Noclip.Velocity = Vector3.new(0, 0, 0)
                end
                if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid"):GetState() == Enum.HumanoidStateType.Seated then
                    game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Jump = true
                    game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Sit = false
                end
            else
                if game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip") then
                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip"):Destroy()
                end
            end
        end, function() end)
    end
end)

task.spawn(function() -- Auto Equip Weapon
    while wait(0.5) do
        xpcall(function()
            if getgenv().AutoEquipWeapon and (getgenv().AutoFarm or getgenv().AutoFarmNearByMobs or getgenv().AutoFarmMobsSelect or getgenv().AutoYellowBlade or getgenv().AutoFarmBossSelect or getgenv().AutoPurpleKatana or (getgenv().AutoFarmLevelAbility and getgenv().SelectAbility ~= "Flash Step") or getgenv().AutoAbility or getgenv().AutoRaid or getgenv().AutoPortal) then
                for _, tool in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                    if tool:IsA("Tool") and tool.ToolTip then
                        if tool.ToolTip == getgenv().SelectWeapon then
                            EquipToolByName(tool.Name)
                        end
                    end
                end
            end
        end, function() end)
    end
end)

task.spawn(function() -- Auto Use Skills
    while wait(0.1) do
        xpcall(function()
            if getgenv().AutoUseSkills and (UseSkillAutoFarm or UseSkillNearByMobs or UseSkillMobSelect or UseSkillAutoSword or UseSkillSelectBoss or  UseSkillAura or UseSkillRaid or UseSkillMemeBeast or UseSkillPortal) then
                local player = game.Players.LocalPlayer
                local character = player.Character

                for _, tool in next, character:GetChildren() do
                    if tool:IsA("Tool") then
                        for _, skill in next, Skills do
                            if table.find(getgenv().SelectSkills, skill) then
                                simulateKeyPress(skill)
                            end
                        end
                    end
                end
            end
        end, function() end)
    end
end)

game:GetService("Players").LocalPlayer.Idled:connect(function() -- Anti Afk
    game:GetService(game:GetService("VirtualUser"):Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame))
    wait(1)
    game:GetService(game:GetService("VirtualUser"):Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame))
end)

