-- Stats Settings GUI
do

    Stats:Label("Stats", 1)

    local SliderPoint = Stats:Slider("Points", 1, 1, 1000, 1, function(value)
        getgenv().Point = value
    end)

    local AutoUpStats = Stats:Toggle("Auto Upstats", 1, false, function(value)
        getgenv().AutoUpStats = value
    end)



    local SelectSkillsMultiDropDown = Stats:MultiDropdown("Stats Selected", 1, {"Melee", "Health", "Weapon", "Power"}, function(value)
        local SelectStats = {}
        for _, stat in ipairs(value) do
            table.insert(SelectStats, stat)
        end
        getgenv().SelectStats = SelectStats
    end)



    Stats:Button("Refresh Stats", 1, function()
        game:GetService("ReplicatedStorage"):WaitForChild("OtherEvent"):WaitForChild("MainEvents"):WaitForChild("StatsFunction"):InvokeServer({["Action"] = "ResetStats"})
        
    end)

    if getgenv().Configs then
    SliderPoint:SetValue(getgenv().Configs["Points"])
    AutoUpStats:SetValue(getgenv().Configs["Auto Upstats"])
    SelectSkillsMultiDropDown:SetValue(getgenv().Configs["Stats Selected"])
    else
    SliderPoint:SetValue(1)
    end
end

