local function updateFarmPosition()
    local farmDistance = getgenv().Distance
    if getgenv().SelectMethod == "Upper" then
        getgenv().FarmPositionCFrame = CFrame.new(0, farmDistance, 0) * CFrame.Angles(math.rad(-90), 0, 0)
    elseif getgenv().SelectMethod == "Behind" then
        getgenv().FarmPositionCFrame = CFrame.new(0, 0, farmDistance)
    elseif getgenv().SelectMethod == "Below" then
        getgenv().FarmPositionCFrame = CFrame.new(0, -farmDistance, 0) * CFrame.Angles(math.rad(90), 0, 0)
    end
end

local function updateWeapon(value)
    if value == "Melee" then
        Weapon = "Fighting Style"
    elseif value == "Sword" then
        Weapon = "Weapon"
    elseif value == "Power Fruit" then
        Weapon = "Power"
    end
    getgenv().SelectWeapon = Weapon
end


-- Farm Settings GUI
do
    FarmSettings:Label("Farm Settings", 1)
    local SelectMethod = FarmSettings:Dropdown("Method Selected", 1, { "Upper", "Behind", "Below" }, function(value)
        getgenv().SelectMethod = value
        updateFarmPosition()
    end)

    local SearchWeapon = FarmSettings:Dropdown("Weapon Selected", 1, {"Melee","Sword","Power Fruit"}, function(value)
        getgenv().SelectWeapon = Weapon
        updateWeapon(value)
    end)

    local SelectSkills = FarmSettings:MultiDropdown("Skills Selected", 1, Skills, function(value)
        getgenv().SelectSkills = value
    end)

    local DistanceFromMob = FarmSettings:Slider("Distance From Mob", 1, 0, 25, 15, function(value)
        getgenv().Distance = value
        updateFarmPosition()
    end)

    FarmSettings:Line(1)

    FarmSettings:Label("Boss Status", 1)

    local  MemeBeast = FarmSettings:Label("Meme Beast : ðŸ”´", 1,13)

    local LordSus = FarmSettings:Label("Lord Sus : ðŸ”´", 1,13)

    local EvilNoob = FarmSettings:Label("Evil Noob : ðŸ”´", 1,13)

    local GiantPumpkin = FarmSettings:Label("Giant Pumpkin : ðŸ”´", 1,13)

    local function updateMonsterStatus(monsterName, statusLabel)
        if game:GetService("Workspace").Monster:FindFirstChild(monsterName) or game:GetService("ReplicatedStorage").MonsterSpawn:FindFirstChild(monsterName) then
            statusLabel:Set(monsterName .. " : ðŸŸ¢")
        else
            statusLabel:Set(monsterName .. " : ðŸ”´")
        end
    end
    
    local function updateAllMonsters()
        xpcall(function()
            updateMonsterStatus("Meme Beast", MemeBeast)
            updateMonsterStatus("Lord Sus", LordSus)
            updateMonsterStatus("Evil Noob", EvilNoob)
            updateMonsterStatus("Giant Pumpkin", GiantPumpkin)
        end, function() end)
    end
    
    local function onChildAddedOrRemoved()
        updateAllMonsters()
    end
    
    game:GetService("Workspace").Monster.ChildAdded:Connect(onChildAddedOrRemoved)
    game:GetService("Workspace").Monster.ChildRemoved:Connect(onChildAddedOrRemoved)
    game:GetService("ReplicatedStorage").MonsterSpawn.ChildAdded:Connect(onChildAddedOrRemoved)
    game:GetService("ReplicatedStorage").MonsterSpawn.ChildRemoved:Connect(onChildAddedOrRemoved)
    
    updateAllMonsters()
    

    -- 2
    

    FarmSettings:Label("Auto Farm Settings", 2)

    local AutoBringMob = FarmSettings:Toggle("Auto Bring Mob", 2, false, function(value)
        getgenv().BringMob = value
    end)

    local AutoEquipWeapon = FarmSettings:Toggle("Auto Equip Weapon", 2, false, function(value)
        getgenv().AutoEquipWeapon = value
    end)

    local AutoUseSkills = FarmSettings:Toggle("Auto Use Skills", 2, false, function(value)
        getgenv().AutoUseSkills = value
    end)

    FarmSettings:Line(2)

    FarmSettings:Label("Auto Settings", 2)

    local AutoEnableAura = FarmSettings:Toggle("Auto Enable Aura", 2, false, function(value)
        getgenv().AutoEnableAura = value
    end)

    local AutoEnableInstinct = FarmSettings:Toggle("Auto Enable Instinct", 2, false, function(value)
        getgenv().AutoEnableInstinct = value
    end)

    local AutoEnableRace = FarmSettings:Toggle("Auto Enable Race", 2, false, function(value)
        getgenv().AutoEnableRace = value
    end)

    -- Apply Saved Configs
    if getgenv().Configs then
        AutoEquipWeapon:SetValue(getgenv().Configs["Auto Equip Weapon"])
        SelectMethod:SetValue(getgenv().Configs["Select Method"])
        SearchWeapon:SetValue(getgenv().Configs["Select Weapon"])
        AutoBringMob:SetValue(getgenv().Configs["Auto Bring Mob"])
        AutoEnableAura:SetValue(getgenv().Configs["Auto Enable Aura"])
        AutoEnableInstinct:SetValue(getgenv().Configs["Auto Enable Instinct"])
        AutoEnableRace:SetValue(getgenv().Configs["Auto Enable Race"])
        SelectSkills:SetValue(getgenv().Configs["Select Skills"])
        DistanceFromMob:SetValue(getgenv().Configs["Distance From Mob"])
        AutoUseSkills:SetValue(getgenv().Configs["Auto Use Skills"])
    else
        SelectMethod:SetValue("Upper")
        DistanceFromMob:SetValue(8)
        SelectSkills:SetValue({ "Z", "X" })
    end
end

