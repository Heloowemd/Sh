getgenv().Configs = {
    ["Team"] = "Floppa", -- Floppa, Cheems

    -- FarmSettings
    ["Auto Equip Weapon"] = true,            -- true, false
    ["Auto Use Skills"] = false,             -- true, false
    ["Select Method"] = "Upper",             -- Upper, Behind, Below
    ["Select Weapon"] = "Melee",             -- Melee,Sword,Power Fruit
    ["Auto Bring Mob"] = false,              -- true, false
    ["Auto Enable Aura"] = false,            -- true, false
    ["Auto Enable Instinct"] = false,        -- true, false
    ["Auto Enable Race"] = false,            -- true, false
    ["Select Skills"] = { "Z", "X" },        -- Z,X,C,V
    ["Distance From Mob"] = 8,               -- 1 - 25

    -- General
    ["AutoFarm Level"] = false,              -- true, false
    ["AutoFarm Near Mobs"] = false,         -- true, false
    ["Farm Nearby Mobs Range"] = 500,        -- 1-5000
    ["Mobs Selected"] = { "Floppa", "Golden Floppa" }, -- Floppa, Golden Floppa and more
    ["AutoFarm Select Mobs"] = false,       -- true, false
